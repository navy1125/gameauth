<?xml version="1.0" encoding="GB2312"?>
<!--所有各种服务器参数,这里只放每个区都不同的配置-->
<Zebra>
	<global>
		<!-- Use the specified local network device. -->
		<mysql encode="no">mysql://root:123@127.0.0.1:3306/MonitorServer</mysql>
		<ifname>eth1</ifname>
	</global>
	<AuthServer>
		<!--MySQL database connection[mysql://user:passwd@host:port/dbName]-->
		<mysql encode="no">mysql://root:123@127.0.0.1:3306/MonitorServer</mysql>
		<mysql_checkname encode="no">mysql://root:123@127.0.0.1:3306/MonitorServer</mysql_checkname>
		<key>key.xml</key>
		<port>8000</port>
		<loginServerList>loginServerList.xml</loginServerList>
		<zqb_zoneid_10000>10000</zqb_zoneid_10000>
	</AuthServer>
</Zebra>
